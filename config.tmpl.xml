		<tizen:icon section="Account">company-logo-72x72.png</tizen:icon>
		<tizen:icon section="AccountSmall">company-logo-45x45.png</tizen:icon>
		<tizen:display-name xml:lang="en-gb">< COMPANY NAME ></tizen:display-name>
